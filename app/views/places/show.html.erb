<p id="notice"><%= notice %></p>

<h2><%= @place.name %></h2>
<br>

<%= @place.street %>
<br>

<%= @place.zip + ' ' + @place.city %>
<br>

<%- osoite = "https://www.google.com/maps/embed/v1/place?q=" + ERB::Util.url_encode("#{@place.name} #{@place.street} #{@place.city} #{@place.country}") + "&key=#{ENV['GMAPS']}" %>

<iframe width="600" height="450" frameborder="0" style="border:0" src="<%= osoite %>" allowfullscreen></iframe>
